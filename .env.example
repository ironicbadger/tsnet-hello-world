# Tailscale Authentication Key
# Generate at: https://login.tailscale.com/admin/settings/keys
TS_AUTHKEY=tskey-auth-example

# Enable Tailscale Serve (makes app accessible on tailnet)
TS_ENABLE_SERVE=true

# Enable Tailscale Funnel (makes app publicly accessible)
# WARNING: This will expose your app to the public internet
TS_ENABLE_FUNNEL=false